{
	"name": "electronjs-template",
	"version": "1.1.1",
	"description": "ElectronJS Template",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"repository": "https://github.com/sundowatch/electronjs-template",
	"keywords": [
		"electron",
		"javascript",
		"template"
	],
	"build": {
		"appId": "org.truncgil.electronjstemplate",
		"productName": "ElectronJSTemplate",
		"copyright": "Copyright © 2020 Ibrahim Sanduvaç",
		"directories": {
			"buildResources": "resources"
		},
		"win": {
			"target": [
				"nsis",
				"msi"
			],
			"asar": true,
			"icon": "resources/icon.ico",
			"publisherName": "Ibrahim Sanduvaç",
			"legalTrademarks": "(c) 2020 truncgil.com"
		}
	},
	"author": "Ibrahim Sanduvaç",
	"license": "MIT",
	"devDependencies": {
		"electron": "^15.5.5",
		"electron-builder": "^22.6.1"
	},
	"dependencies": {
		"bootstrap": "^4.5.0",
		"jquery": "^3.5.1",
		"popper": "^1.0.1"
	}
}
